     RowBox[{"Permutations", "[", 
      RowBox[{"{", 
       RowBox[{"1", ",", "2", ",", "3"}], "}"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "columnOrder", " ", "is", " ", "equal", " ", "to", " ", "rowOrder", " ", 
      "but", " ", "with", " ", "morphogen", " ", "always", " ", "as", " ", 
