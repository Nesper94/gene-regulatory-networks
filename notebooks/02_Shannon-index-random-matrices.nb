    RowBox[{"columnOrder", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"Prepend", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"rowOrder", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "+", "1"}], ",", "1"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", 
         RowBox[{"Length", "[", "rowOrder", "]"}]}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"elimination", "=", "randomMatrices"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "Create", " ", "empty", " ", "list", " ", "that", " ", "will", " ", 
      "contain", " ", "the", " ", "topologies", " ", "and", " ", "its", " ", 
      "frequence"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{"motifs", "=", 
     RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"While", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Length", "[", "elimination", "]"}], ">", "0"}], ",", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"randomMatrix", "=", 
         RowBox[{"elimination", "[", 
          RowBox[{"[", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{"1", ",", 
              RowBox[{"Length", "[", "elimination", "]"}]}], "}"}], "]"}], 
           "]"}], "]"}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
         "Generate", " ", "isomorphs", " ", "of", " ", "the", " ", "matrix"}],
          "*)"}], "\[IndentingNewLine]", 
        RowBox[{"isomorphs", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"randomMatrix", "[", 
            RowBox[{"[", 
             RowBox[{
              RowBox[{"rowOrder", "[", 
               RowBox[{"[", "i", "]"}], "]"}], ",", 
              RowBox[{"columnOrder", "[", 
               RowBox[{"[", "i", "]"}], "]"}]}], "]"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", 
             RowBox[{"Length", "[", "rowOrder", "]"}]}], "}"}]}], "]"}]}], 
        ",", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
         "Count", " ", "times", " ", "this", " ", "matrix", " ", "or", " ", 
          "its", " ", "isomorphs", " ", "appear"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"frecMatriz", "=", 
         RowBox[{"Count", "[", 
          RowBox[{"elimination", ",", 
           RowBox[{"Alternatives", "@@", "isomorphs"}]}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
         "Delete", " ", "matrix", " ", "and", " ", "its", " ", "isomorphs", 
          " ", "from", " ", "the", " ", "list", " ", "of", " ", "matrices"}], 
         "*)"}], "\[IndentingNewLine]", 
        RowBox[{"elimination", "=", 
         RowBox[{"DeleteCases", "[", 
          RowBox[{"elimination", ",", 
           RowBox[{"Alternatives", "@@", "isomorphs"}]}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
         "Add", " ", "matriz", " ", "and", " ", "its", " ", "frequency", " ", 
          "to", " ", "the", " ", "\"\<motifs\>\"", " ", "list"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"AppendTo", "[", 
         RowBox[{"motifs", ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"isomorphs", "[", 
             RowBox[{"[", "1", "]"}], "]"}], ",", "frecMatriz"}], "}"}]}], 
         "]"}]}], "}"}]}], "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"Sort", " ", "motifs", " ", "by", " ", "frequency"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"motifs", "=", 
